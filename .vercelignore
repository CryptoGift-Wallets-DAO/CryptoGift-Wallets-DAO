# Vercel Ignore - Prevent large files from being included in serverless functions

# Package manager stores (CRITICAL - these are huge!)
.pnpm-store
node_modules/.pnpm
node_modules/.cache

# Development directories
.git
.github
scripts
test
tests
hardhat.config.js
hardhat.config.production.js

# Build artifacts
artifacts
cache
coverage
typechain-types
dist
build

# Documentation (heavy images)
*.PNG
*.png
*.jpg
*.jpeg
*.gif
*.ico
!app/icon.png
!public/favicon.ico
!public/apeX.png
!public/apeX22.PNG

# Contracts (not needed for runtime)
/contracts
deployments/*
!deployments/deployment-base-latest.json

# Backup files (prevent bundling in serverless functions)
backups
*.backup
*.bak
app/api/*/route.full.ts.backup

# Frontend/Backend folders (if not needed)
frontend
ranking-frontend
ranking-backend
bots

# Config files
*.config.js
*.config.ts
!next.config.js
!next.config.mjs
!tailwind.config.js
!tailwind.config.ts
!postcss.config.js
!sentry.server.config.js
!sentry.edge.config.js

# Misc
*.log
*.lock
!pnpm-lock.yaml
.env*
!.env.production
*.md
!CLAUDE.md
!README.md
!docs/PLAN_DESARROLLO_COMPLETO.md